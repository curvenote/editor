{
  "name": "@curvenote/editor",
  "version": "0.15.1",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "prosemirror",
    "explorable explanations",
    "writing"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/curvenote/editor.git"
