  "bugs": {
    "url": "https://github.com/curvenote/editor/issues"
  },
  "homepage": "https://curvenote.dev/editor",
  "scripts": {
    "emoji": "node bin/createEmoji.js; mv bin/emoji.json src/store/suggestion/results/emoji.json",
    "clean": "rm -rf dist; rm src/store/suggestion/results/emoji.json || true;",
    "compile": "yarn run clean; yarn run emoji; tsc;",
    "build": "yarn run compile; yarn run css:build; webpack --config webpack.prod.js",
    "prepublishOnly": "yarn run build; yarn run css:build;",
    "test": "jest",
    "css:copy": "mkdir -p dist; mkdir -p styles/article; cp node_modules/@curvenote/article/dist/styles/_* styles/article; cp node_modules/prosemirror-codemark/dist/codemark.css styles",
    "css:watch": "yarn run css:copy; sass --watch styles/index.scss dist/editor.css",
    "css:build": "yarn run css:copy; sass styles/index.scss dist/editor.css",
    "test:watch": "jest --watchAll",
    "test:clear": "jest --clearCache",
    "start": "webpack serve --liveReload --open --config webpack.dev.js",
    "dev": "webpack --config webpack.dev.js",
    "dev:schema": "cd ../schema; yalc publish; cd ../editor; yalc link @curvenote/schema",
    "dev:autocomplete": "cd ../prosemirror-autocomplete; yalc publish; cd ../editor; yalc link prosemirror-autocomplete",
    "lint": "eslint \"src/**/*.ts\" -c .eslintrc.json",
    "lint:format": "prettier --check \"src/**/*.tsx\" \"src/**/*.ts\"",
    "lint:format:fix": "prettier --write \"src/**/*.tsx\" \"src/**/*.ts\"",
    "watch": "tsc -w -p ."
  },
  "dependencies": {
    "codemirror": "^5.63.3",
    "katex": "^0.13.19",
    "lodash.debounce": "^4.0.8",
    "lodash.isequal": "^4.5.0",
    "lodash.throttle": "^4.1.1",
    "prosemirror-autocomplete": "^0.3.0",
    "prosemirror-codemark": "^0.3.4",
    "prosemirror-commands": "^1.1.12",
    "prosemirror-dropcursor": "^1.4.0",
    "prosemirror-gapcursor": "^1.2.0",
    "prosemirror-history": "^1.2.0",
    "prosemirror-inputrules": "^1.1.3",
    "prosemirror-keymap": "^1.1.5",
    "prosemirror-tables": "^1.1.1",
    "prosemirror-utils": "^0.9.6",
    "scroll-into-view-if-needed": "^2.2.28",
    "uuid": "^8.3.2"
  },
